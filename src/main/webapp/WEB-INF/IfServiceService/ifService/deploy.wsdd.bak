<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from IfServiceService WSDL service -->

  <service name="IfService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ifService"/>
      <parameter name="wsdlServiceElement" value="IfServiceService"/>
      <parameter name="schemaQualified" value="http://ifService"/>
      <parameter name="wsdlServicePort" value="IfService"/>
      <parameter name="className" value="ifService.IfServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="IfService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getName" qname="operNS:getName" xmlns:operNS="http://ifService" returnQName="retNS:getNameReturn" xmlns:retNS="http://ifService" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://ifService" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getType" qname="operNS:getType" xmlns:operNS="http://ifService" returnQName="retNS:getTypeReturn" xmlns:retNS="http://ifService" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://ifService" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="login" qname="operNS:login" xmlns:operNS="http://ifService" returnQName="retNS:loginReturn" xmlns:retNS="http://ifService" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://ifService" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://ifService" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="selectProduct" qname="operNS:selectProduct" xmlns:operNS="http://ifService" returnQName="retNS:selectProductReturn" xmlns:retNS="http://ifService" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:idProduct" xmlns:pns="http://ifService" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getPrice" qname="operNS:getPrice" xmlns:operNS="http://ifService" returnQName="retNS:getPriceReturn" xmlns:retNS="http://ifService" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://ifService" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:currency" xmlns:pns="http://ifService" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="signUp" qname="operNS:signUp" xmlns:operNS="http://ifService" returnQName="retNS:signUpReturn" xmlns:retNS="http://ifService" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://ifService" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://ifService" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:firstName" xmlns:pns="http://ifService" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:lastName" xmlns:pns="http://ifService" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getAllProduct" qname="operNS:getAllProduct" xmlns:operNS="http://ifService" returnQName="retNS:getAllProductReturn" xmlns:retNS="http://ifService" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="buyProduct" qname="operNS:buyProduct" xmlns:operNS="http://ifService" returnQName="retNS:buyProductReturn" xmlns:retNS="http://ifService" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:idProducts" xmlns:pns="http://ifService" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:price" xmlns:pns="http://ifService" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:idUser" xmlns:pns="http://ifService" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="balanceValue" qname="operNS:balanceValue" xmlns:operNS="http://ifService" returnQName="retNS:balanceValueReturn" xmlns:retNS="http://ifService" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://ifService" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="buyProduct getName getType balanceValue login getAllProduct signUp getPrice selectProduct"/>

  </service>
</deployment>
